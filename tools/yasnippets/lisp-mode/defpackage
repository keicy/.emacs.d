(in-package :cl-user)
(require : #P"")
(provide :${1:name})
(defpackage $1
  (:use :cl)
  (:shadow {:symbol} )
  (:shadowing-import-from {:package} {:symbol}... )
  (:import-from {:package} {:symbol}... )
  (:export :)
)
(in-package :$1)


